
<div class="content-page">
  <!-- Start content -->
  <div class="content">
    <div class="container">

      <!-- INJECT DASHBOARD VIEW  -->

      <div class="row">
        <div class="col-sm-10">
          <div class="wrapper-page signup-signin-page">
            <div class="card-box">
              <div class="panel-heading">
                <h3 class="text-center"> <strong class="text-custom">BANK <i class="md md-album"></i> REGISTRATION </strong></h3>
              </div>

              <div class="panel-body">
                <div class="row">
                  <div class="col-lg-6">
                    <!-- ADD SUCCESS/ERROR ALERTS HERE -->
                    <h3></h3>
                  </div>



                </div>
                <div class="row">
                  <div class="col-lg-12">
                    <div class="p-20">
                      <%= form_for((@bank), class: 'form-horizontal m-t-20 ng-pristine ng-valid ng-valid-email') do |f|%>
                      <% if @bank.errors.any? %>
                      <div id="error_explanation">
                        <h2><%= pluralize(@bank.errors.count, "error") %> prohibited this bank from being saved:</h2>

                        <ul>
                          <% @bank.errors.full_messages.each do |message| %>
                          <li><%= message %></li>
                          <% end %>
                        </ul>
                      </div>
                      <% end %>

                      <div class="input-group m-t-10">
                        <%= f.text_field :name, placeholder: 'Name', class: "form-control ng-pristine ng-untouched ng-valid"%>
                        <span class="input-group-addon"><i class="fa fa-institution"></i></span>
                      </div>

                      <div class="input-group m-t-10">
                        <%= f.text_field :bank_type, placeholder: 'Bank Type', class: "form-control ng-pristine ng-untouched ng-valid"%>
                        <span class="input-group-addon"><i class="md md-vpn-key"></i></span>
                      </div>

                      <div class="input-group m-t-10">
                        <%= f.text_field :reg_no, placeholder: 'Registration Number', class: "form-control ng-pristine ng-untouched ng-valid"%>
                        <span class="input-group-addon"><i class="fa fa-envelope-o"></i></span>
                      </div>

                      <div class="input-group m-t-10">
                        <%= f.date_select :founded, placeholder: 'Founded', class: "form-control ng-pristine ng-untouched ng-valid"%>
                        <span class="input-group-addon"><i class="fa  fa-globe"></i></span>
                      </div>


                      <div class="input-group m-t-10">
                        <%= f.text_field :headquarters, placeholder: 'Headquarters', class: "form-control ng-pristine ng-untouched ng-valid"%>
                        
                        <span class="input-group-addon"><i class="md md-contacts"></i></span>
                      </div>

                      <div class="input-group m-t-10">
                        <%= f.text_field :ceo, placeholder: 'CEO', class: "form-control ng-pristine ng-untouched ng-valid"%>
                        <span class="input-group-addon"><i class="md md-settings-cell"></i></span>
                      </div>

                      <div class="input-group m-t-10">
                        <%= f.text_area :services, placeholder: 'Services', class: "form-control ng-pristine ng-untouched ng-valid"%>
                        <span class="input-group-addon"><i class="md md-settings-cell"></i></span>
                      </div>

                      <div class="input-group m-t-10">
                        <%= f.text_field :email, placeholder: 'Email', class: "form-control ng-pristine ng-untouched ng-valid"%>
                        <span class="input-group-addon"><i class="md md-settings-cell"></i></span>
                      </div>

                      <div class="input-group m-t-10">
                        <%= f.text_field :website, placeholder: 'website', class: "form-control ng-pristine ng-untouched ng-valid"%>
                        <span class="input-group-addon"><i class="md md-settings-cell"></i></span>
                      </div>

                      <div class="form-group text-right m-t-20 m-b-0">
                        <div class="col-xs-12">
                          <%= f.submit class: "btn btn-pink text-uppercase waves-effect waves-light w-sm" %>
                        </div>
                      </div>
                      <% end %>
                    </div>
                  </div>

                    <!-- <div class="bootstrap-filestyle input-group">
                     <input type="text" class="form-control " placeholder="Bank Logo" disabled=""> 
                     <span class="group-span-filestyle input-group-btn" tabindex="0">
                       <label for="filestyle-9" class="btn btn-default ">
                        <span class="icon-span-filestyle glyphicon glyphicon-folder-open"></span> 
                        <span class="buttonText">Choose file</span>
                      </label>
                    </span>
                  </div> -->




                  



                </div>

              </div>
            </div>

          </div>
        </div>
      </div>


    </div> <!-- container -->

  </div> <!-- content -->


